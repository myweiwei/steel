.container{
  background-color: #fff;
  overflow: hidden;
}
.topPicture{
  width:100%;
  height:450rpx;
}
.enterpriseType{
  width:95%;
  overflow:hidden;
  white-space:nowrap;
  text-overflow: ellipsis;
}
.top{
  display: flex;
  flex-direction:row;
  flex-wrap: nowrap;
  justify-content: flex-start;
  align-items: flex-start;
  padding-top: 20rpx;
  font-size: 26rpx;
  color:#323232;
}
.top .logo{
  width: 140rpx;
  height: 130rpx;
  border: 1rpx solid #f5f5f5;
  border-radius: 20rpx;
}
.fontSizeMiddle{
  font-size: 25rpx;
}
.top .left{
  margin-left: 20rpx;
  width: 150rpx;
  box-sizing: border-box;
}
.top .middle{
  width:440rpx;
}


/*经营产品*/
.goodsBox{
   width:46%;
   height:450rpx;
   margin-top: 20rpx;
   margin-left: 20rpx;
   background:#FFFFFF;
   border-radius:8rpx;
   box-sizing: border-box;

}
.goodsBox image{
  width:100%;
  height:280rpx;
  background:#FFFFFF;
  border-radius:8rpx 8rpx 0rpx 0rpx;
}
.footer {
  background: transparent;
}
.footer van-button {
  width:48%;
  margin:0 1%;
}
.footer van-button button {
  width:100%;
  border-radius:15rpx;
}
.van-button__icon {
  font-size:33rpx;
}