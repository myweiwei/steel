.container{
  background-color: #f5f5f5;
  height: 100%;
}
.enterpriseList{
  margin: 0px auto;
  background:#FFFFFF;
  margin-bottom:20rpx;
  border-radius: 20rpx;
}
.enterpriseType{
  width:95%;
  overflow:hidden;
  white-space:nowrap;
  text-overflow: ellipsis;
}
.top{
  display: flex;
  flex-direction:row;
  flex-wrap: nowrap;
  justify-content: space-between;
  align-items: flex-start;
  padding-top: 20rpx;
  font-size: 26rpx;
  color:#323232;
  margin-top:10rpx;
}
.top .left{
  margin-left: 20rpx;
  width: 150rpx;
  box-sizing: border-box;
}
.top .middle{
  width:410rpx;
  margin-left: 15rpx;

}
.top .right{
  color:#6f6f6f;
  box-sizing: border-box;
}
.fontSizeMiddle{
  font-size: 25rpx;
}
.top .see{
  width:30rpx; height:20rpx;
}
.top .follow{
  width:22rpx; height:22rpx;
}
.top .logo{
  width: 140rpx;
  height: 130rpx;
  border: 1rpx solid #f5f5f5;
  border-radius: 20rpx;
}
.bottom{
  margin-top: 10rpx;
}
.bottom .goods{
  width:170rpx;
  height:140rpx;
  background-color: #f8f7f6;
  border: 1rpx solid #f5f5f5;
  border-radius: 20rpx;
}
.bottom .goodsName{
  color:#323232; 
  font-size:24rpx;
  overflow:hidden;
  width:180rpx;
  white-space:nowrap;
  text-overflow: ellipsis;
}
.bottom .price{
  color:#dd000a;
  font-size:24rpx;
}
.van-search {
  padding:0px!important;
  background:transparent!important;
}
.van-search__content{
  background: rgba(255,255,255,0.4)!important;
}
.search_ipt,.van-icon-search {
  color:#fff!important;
}
.phholder {
  color:#fff;
}