/* pages/mine/regEnterprise/regEnterpriseSuccess/regEnterpriseSuccess.wxss */
.regSuc_top {
  text-align: center;
  border-bottom:4rpx solid #f2f2f2;
  margin:150rpx 20rpx 0;
  padding-bottom:120rpx;
  box-sizing: border-box;
}
.regSuc_top image {
  width:200rpx;
  height:200rpx;
}
.regSuc_top .title {
  font-size:46rpx;
  color:#1c8cfa;
  margin-left:20rpx;
}
.regSuc_cen {
  margin-top:50rpx;
  color:#000000;
}
.regSuc_cen .info {
  line-height: 50rpx;
  font-size:28rpx;
}
.regSuc_foot {
  position: fixed;
  bottom:80rpx;
  width:80%;
  text-align: center;
  left:50%;
  transform: translateX(-50%)
}
.regSuc_foot button {
  background: #1c8cfa!important;
  border:none!important;
  color:#Fff!important;
}