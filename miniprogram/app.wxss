/**app.wxss**/
page {
  background: #fff;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  height:100%;
} 
.flexStart,.flexBetween,.flexAround,.flexCenter{
  display: flex;
  align-items: center;
  justify-content: flex-start;
}
.flexBetween {
  justify-content: space-between;
}
.flexAround {
  justify-content: space-around;
}
.flexCenter {
  justify-content: center;
}
.footTab {
  width:100%;
  height:90rpx;
  position: fixed;
  left:0;
  bottom:0;
  box-shadow: 0 0 2rpx 2rpx #e7e7e7;
  background: #fff;
}
.strong {
  font-weight: 700;
  margin:0 5rpx;
}
.normal_title {
  width:100%;
  font-size:30rpx;
  color:#1e0e06;
}
.splite {
  width:100%;
  height:10rpx;
  background: #f5f5f5;
}
::-webkit-scrollbar {
  width: 0!important;
  height:0!important;
  color:transparent!important;
}
.m-r-10 {
  margin-right:10rpx;
}
.f-40 {
  font-size:40rpx;
}
.color-gray {
  color:#666;
}
.color-orange {
  color:#fb8c3f!important;
}
.fonWei {
  font-weight:700;
}


